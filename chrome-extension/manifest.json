{
    "manifest_version": 2,
    "name": "IJDB",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["scrape.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": ["images/close.png", "images/open.png"],
    "permissions": ["activeTab"]

}